{
    "swagger": "2.0",
    "info": {
        "version": "2.0.0",
        "title": "SUNFISH Blockchain API - Data masking tables",
        "contact": {
            "name": "Md Sadek Ferdous",
            "email": "S.Ferdous@soton.ac.uk"
        }
    },
    "basePath": "/api/datamasking/tables",
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/create": {
            "post": {
                "description": "This endpoint is used to create a table.\n",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Body in JSON",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/create-body"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The response body for a successful response.",
                        "schema": {
                            "$ref": "#/definitions/create-response"
                        }
                    },
                    "400": {
                        "description": "Invalid request, required parameter(s) missing."
                    },
                    "401": {
                        "description": "The operation is not allowed (unauthorised access, the token is invalid, etc.)."
                    }
                }
            }
        },
        "/read": {
            "post": {
                "description": "This endpoint is used to retrieve a stored table by its unique ID.\n",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Body in JSON",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/read-body"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The response body for a successful response.",
                        "schema": {
                            "$ref": "#/definitions/read-add-response"
                        }
                    },
                    "400": {
                        "description": "Invalid request, required parameter(s) missing"
                    },
                    "401": {
                        "description": "The operation is not allowed (unauthorised access, the token is invalid, etc.)."
                    }
                }
            }
        },
        "/delete": {
            "post": {
                "description": "This endpoint is used to destroy a table by its unique ID. \n",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Body in JSON",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/delete-body"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The response body for a successful response.",
                        "schema": {
                            "$ref": "#/definitions/response"
                        }
                    },
                    "400": {
                        "description": "Invalid request, required parameter(s) missing"
                    },
                    "401": {
                        "description": "The operation is not allowed (unauthorised access, the token is invalid, etc.)."
                    }
                }
            }
        },
        "/add": {
            "post": {
                "description": "This endpoint is used to add a piece to a table.\n",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Body in JSON",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/add-body"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The response body for a successful response.",
                        "schema": {
                            "$ref": "#/definitions/read-add-response"
                        }
                    },
                    "400": {
                        "description": "Invalid request, required parameter(s) missing"
                    },
                    "401": {
                        "description": "The operation is not allowed (unauthorised access, the token is"
                    }
                }
            }
        },
        "/deletepiece": {
            "post": {
                "description": "This endpoint is used to delete a piece from a table.\n",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Body in JSON",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/delete-piece-body"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The response body for a successful response.",
                        "schema": {
                            "$ref": "#/definitions/response"
                        }
                    },
                    "400": {
                        "description": "Invalid request, required parameter(s) missing"
                    },
                    "401": {
                        "description": "The operation is not allowed (unauthorised access, the token is"
                    }
                }
            }
        }
    },
    "definitions": {
        "create-body": {
            "type": "object",
            "description": "HTTP body defining the register operation for the /create endpoint.",
            "required": [
                "requestorID",
                "token",
                "timeStamp",
                "dataID"
            ],
            "properties": {
                "requestorID": {
                    "type": "string",
                    "description": "Identifier of the requesting entity."
                },
                "token": {
                    "type": "string",
                    "description": "A crypto token to validate if the entity is allowed to perform the requested operation."
                },
                "timeStamp": {
                    "type": "string",
                    "description": "The timestamp in the following format - \"2006-01-02 15:04:05\"."
                },
                "dataID": {
                    "type": "string",
                    "description": "Identifier of the dataset that the encrypted masking table relates to."
                }
            }
        },
        "read-body": {
            "type": "object",
            "description": "HTTP body defining the read operation for the /read endpoint",
            "required": [
                "requestorID",
                "token",
                "tableID"
            ],
            "properties": {
                "requestorID": {
                    "type": "string",
                    "description": "Identifier of the requesting entity."
                },
                "token": {
                    "type": "string",
                    "description": "A crypto token to validate if the entity is allowed to perform the requested operation."
                },
                "tableID": {
                    "type": "string",
                    "description": "Identifier of an encrypted masking table."
                }
            }
        },
        "delete-body": {
            "type": "object",
            "description": "HTTP body defining the delete operation for the /delete endpoint",
            "required": [
                "requestorID",
                "token",
                "tableID"
            ],
            "properties": {
                "requestorID": {
                    "type": "string",
                    "description": "Identifier of the requesting entity."
                },
                "token": {
                    "type": "string",
                    "description": "A crypto token to validate if the entity is allowed to perform the requested operation."
                },
                "tableID": {
                    "type": "string",
                    "description": "Identifier of an encrypted masking table."
                }
            }
        },
        "delete-piece-body": {
            "type": "object",
            "description": "HTTP body defining the delete operation for the /deletepiece endpoint",
            "required": [
                "requestorID",
                "token",
                "tableID",
                "pieceID"
            ],
            "properties": {
                "requestorID": {
                    "type": "string",
                    "description": "Identifier of the requesting entity."
                },
                "token": {
                    "type": "string",
                    "description": "A crypto token to validate if the entity is allowed to perform the requested operation."
                },
                "tableID": {
                    "type": "string",
                    "description": "Identifier of an encrypted masking table."
                },
                "pieceID": {
                    "type": "string",
                    "description": "identifier of a piece."
                }
            }
        },
        "add-body": {
            "type": "object",
            "description": "HTTP body defining the add operation for the /add endpoint",
            "required": [
                "requestorID",
                "token",
                "tableID",
                "pieces"
            ],
            "properties": {
                "requestorID": {
                    "type": "string",
                    "description": "Identifier of the requesting entity."
                },
                "token": {
                    "type": "string",
                    "description": "A crypto token to validate if the entity is allowed to perform the requested operation."
                },
                "tableID": {
                    "type": "string",
                    "description": "Identifier of an encrypted masking table."
                },
                "pieces": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "pieceID": {
                                "type": "string",
                                "description": "Identifier of a piece."
                            },
                            "position": {
                                "type": "string",
                                "description": "the position where the piece is located in the table."
                            }
                        },
                        "required": [
                            "pieceID",
                            "position"
                        ]
                    }
                }
            }
        },
        "response": {
            "type": "object",
            "description": "HTTP response for a successful operation",
            "required": [
                "message"
            ],
            "properties": {
                "message": {
                    "type": "string",
                    "description": "A message signifying that the operation has been successfully performed."
                }
            }
        },
        "create-response": {
            "type": "object",
            "description": "HTTP response for a successful create operation",
            "required": [
                "tableID"
            ],
            "properties": {
                "tableID": {
                    "type": "string",
                    "description": "The index of the created table which is required to retrieve this particular table."
                }
            }
        },
        "read-add-response": {
            "type": "object",
            "description": "HTTP response for a successful read/add operation",
            "required": [
                "timeStamp",
                "tableID",
                "pieces"
            ],
            "properties": {
                "timeStamp": {
                    "type": "string",
                    "description": "The timestamp in the following format - \"2006-01-02 15:04:05\"."
                },
                "tableID": {
                    "type": "string",
                    "description": "Identifier of an encrypted masking table."
                },
                "pieces": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "pieceID": {
                                "type": "string",
                                "description": "Identifier of a piece."
                            },
                            "position": {
                                "type": "string",
                                "description": "the position where the piece is located in the table."
                            }
                        },
                        "required": [
                            "pieceID",
                            "position"
                        ]
                    }
                }
            }
        }
    }
}
